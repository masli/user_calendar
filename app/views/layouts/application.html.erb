<!DOCTYPE html>
<html>
	<head>
		<title>My app</title>
		<%= csrf_meta_tag %>
		<%= render 'layouts/stylesheets' %>





		<%=javascript_include_tag  'jquery','jquery-ui', 'fullcalendar', 'gcal', 'application' %>
	</head>
	<body>
		<div class="container">
			<%= render 'layouts/header' %>
			<section class="round">
				<% flash.each do |key, value| %>
					<div class="flash <%= key %>"><%= value %></div>
				<% end %>
				<%= yield %>
			</section>
			<%= render 'layouts/footer' %>

		</div>
	</body>
</html>